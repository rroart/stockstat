apiVersion: apps/v1
kind: Deployment
metadata:
  name: pdfred
  labels:
    app: pdfred
spec:
  replicas: 1
  selector:
    matchLabels:
      app: pdfred
  template:
    metadata:
      labels:
        app: pdfred
    spec:
      containers:
      - name: pdfred
        image: pdfred
        #imagePullPolicy: Never
---
apiVersion: v1
kind: Service
metadata:
  name: pdfred
  labels:
    app: pdfred
spec:
  selector:
    app: pdfred
  sessionAffinity: None  
status:
  loadBalancer: {}    
