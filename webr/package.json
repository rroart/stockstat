{
  "name": "react-redux-boilerplate",
  "version": "1.1.1",
  "description": "React Redux Boilerplate is a workflow boilerplate that make life easier for developers by providing a virtual development environment and production ready build process framework out of the box.",
  "browser": {
    "[module-name]": false
  },
  "scripts": {
    "dev": "./node_modules/.bin/cross-env NODE_ENV=production DASHBOARD_PORT=9902 webpack-dashboard -p 9902 -c red -t dashboard -- node bin/commands.js dev",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production node bin/commands.js build",
    "build:stage": "cross-env NODE_ENV=stage node bin/commands.js build",
    "clean": "./node_modules/.bin/rimraf docroot",
    "test": "./node_modules/.bin/jest --no-cache",
    "lint": "node bin/commands.js lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iroy2000/react-redux-boilerplate.git"
  },
  "homepage": "https://github.com/iroy2000/react-redux-boilerplate",
  "author": "Roy Yu <iroy2000@gmail.com>",
  "license": "MIT",
  "engines": {
    "npm": ">=3.10.7",
    "node": ">=6.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@babel/runtime": "^7.26.0",
    "@cyclonedx/webpack-plugin": "^3.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/classnames": "^2.3.4",
    "@types/jest": "^29.5.14",
    "@types/react-responsive": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "assets-webpack-plugin": "^7.1.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.26.0",
    "browser-sync-webpack-plugin": "^2.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "classnames": "^2.5.1",
    "colors": "^1.4.0",
    "concurrently": "^9.1.2",
    "config": "^3.3.12",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "file-loader": "^6.2.0",
    "helper-toolkit-ts": "^1.2.1",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^8.0.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-check-updates": "^17.1.14",
    "postcss-cssnext": "^3.1.1",
    "postcss-loader": "^8.1.1",
    "precss": "^4.0.0",
    "raf": "^3.4.1",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^6.0.1",
    "shelljs": "^0.8.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.7.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dashboard": "^3.0.0",
    "webpack-dev-server": "^5.2.0",
    "webpack-s3-plugin": "^0.1.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "@types/react": "^18.2",
    "@types/react-dom": "^18.2",
    "@types/react-redux": "^7.1.34",
    "history": "^5.3.0",
    "immutable": "^5.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.7",
    "react-datepicker": "^7.6.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.3",
    "react-router-dom": "^7.1.3",
    "react-router-redux": "^4.0.8",
    "react-select": "^5.9.0",
    "react-table": "^7.8.0",
    "react-tooltip": "^5.28.0",
    "redux": "^5.0.1",
    "redux-actions": "^3.0.3",
    "redux-saga": "^1.3.0",
    "reselect": "^5.1.1",
    "styled-components": "^6.1.14"
  },
  "jest": {
    "testEnvironmentOptions": {
      "url": "http://www.SomeAwesome.com"
    },
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/",
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup/jestTestSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "automock": false,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js|jsx|json)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "jsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "webpack-dev-server",
    "postcss",
    "jest",
    "boilerplate",
    "hmr"
  ]
}
