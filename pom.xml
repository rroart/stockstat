    <tweety.version>1.20</tweety.version>
    <!-- Spark needs old version -->
    <scala.binary.version>2.13</scala.binary.version>
    <janino.version>3.1.9</janino.version>
    <jakartaservlet.version>4.0.3</jakartaservlet.version>
