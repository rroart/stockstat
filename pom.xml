    <antlr4.version>4.8</antlr4.version>
  </properties>
  <repositories>
    <repository> 
        <id>repository.spring.milestone</id> 
        <name>Spring Milestone Repository</name> 
        <url>https://repo.spring.io/milestone</url> 
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository> 
        <id>repository.spring.milestone</id> 
        <name>Spring Milestone Repository</name> 
        <url>https://repo.spring.io/milestone</url> 
    </pluginRepository>
  </pluginRepositories>
</project>
